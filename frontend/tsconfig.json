{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@env": ["./env.d.ts"]
    },
    "jsx": "react",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "module": "commonjs",
    "target": "es5",
    "lib": ["esnext", "dom"],
    "moduleResolution": "node"
  },
  "include": ["src", "env.d.ts"]
}
